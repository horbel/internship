@model IEnumerable<SecondTry.Models.Blog>
@{
    Layout = null;
    ViewBag.Title = "Blogs";
}

<h2>Blogs</h2>
<p>
    <a asp-controller="Blogs" asp-action="Create">Create New</a>
</p>

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Url</th>
        </tr>
    </thead>
    <tbody>
        
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.BlogId</td>
                <td>@item.Url</td>
            </tr>
        }
    </tbody>
</table>